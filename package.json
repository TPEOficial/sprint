{
  "name": "sprint-es",
  "version": "0.0.21",
  "description": "Sprint - Quickly API",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./rate-limit": {
      "types": "./dist/types/modules/rate-limit/index.d.ts",
      "require": "./dist/cjs/modules/rate-limit/index.cjs",
      "import": "./dist/esm/modules/rate-limit/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "start": "ts-node src/index.ts",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "dev:example": "npm run build && node example/app.js"
  },
  "repository": {
    "url": "https://github.com/TPEOficial/sprint"
  },
  "keywords": [
    "express",
    "api",
    "sprint",
    "rate-limit"
  ],
  "author": "TPEOficial LLC",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TPEOficial/sprint/issues"
  },
  "homepage": "https://dymo.tpeoficial.com",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "path": "^0.12.7",
    "serve-favicon": "^2.5.1",
    "toolkitify": "^0.0.20"
  },
  "contributors": [
    "TPEOficial (https://github.com/TPEOficial)",
    "FJRG2007 (https://github.com/FJRG2007)"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.7",
    "@types/serve-favicon": "^2.5.7",
    "dotenv": "^17.2.3",
    "jest": "^30.1.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false
  }
}