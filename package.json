{
  "name": "sprint",
  "version": "0.0.18",
  "description": "Sprint - Quickly API",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "exports": {
    "./*": {
      "require": "./dist/cjs/**/*.cjs",
      "import": "./dist/esm/**/*.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && node ./scripts/rename.js",
    "start": "ts-node src/index.ts",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "dev:example": "npm run build && node example/app.js"
  },
  "repository": {
    "url": "https://github.com/TPEOficial/sprint-node"
  },
  "keywords": [
    "cache",
    "caching",
    "toolkit",
    "sprint",
    "rate-limit",
    "server-only",
    "client-only",
    "optimistic-ui"
  ],
  "author": "TPEOficial LLC",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TPEOficial/sprint-node/issues"
  },
  "homepage": "https://dymo.tpeoficial.com",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "path": "^0.12.7",
    "serve-favicon": "^2.5.1"
  },
  "contributors": [
    "TPEOficial (https://github.com/TPEOficial)",
    "FJRG2007 (https://github.com/FJRG2007)"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.7",
    "@types/serve-favicon": "^2.5.7",
    "dotenv": "^17.2.3",
    "jest": "^30.1.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false
  }
}
